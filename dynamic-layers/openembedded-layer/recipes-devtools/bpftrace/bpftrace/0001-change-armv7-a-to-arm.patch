From 57d202fbd55448b66f974a6247c5e1c3b44d68d2 Mon Sep 17 00:00:00 2001
From: pkemb <pkemb@outlook.com>
Date: Thu, 9 Feb 2023 22:28:31 +0800
Subject: [PATCH] change armv7-a to arm

Signed-off-by: pkemb <pkemb@outlook.com>
---
 src/arch/CMakeLists.txt | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/arch/CMakeLists.txt b/src/arch/CMakeLists.txt
index 60fa568c..b3ecf506 100644
--- a/src/arch/CMakeLists.txt
+++ b/src/arch/CMakeLists.txt
@@ -1,6 +1,6 @@
 if(CMAKE_SYSTEM_PROCESSOR STREQUAL "aarch64")
     add_library(arch aarch64.cpp)
-elseif(CMAKE_SYSTEM_PROCESSOR STREQUAL "armv7-a")
+elseif(CMAKE_SYSTEM_PROCESSOR STREQUAL "arm")
     add_library(arch arm.cpp)
 elseif(CMAKE_SYSTEM_PROCESSOR STREQUAL "ppc64" OR
        CMAKE_SYSTEM_PROCESSOR STREQUAL "ppc64le")
